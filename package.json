{
  "name": "@mingchang137/pdf-to-image-node",
  "version": "0.1.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "pdf-to-image-node",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl",
        "universal-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "universal": "napi universal",
    "version": "napi version"
  },
  "optionalDependencies": {
    "pdf-to-image-node-linux-arm64-gnu": "0.1.0",
    "pdf-to-image-node-linux-arm64-musl": "0.1.0",
    "pdf-to-image-node-linux-x64-gnu": "0.1.0",
    "pdf-to-image-node-linux-x64-musl": "0.1.0",
    "pdf-to-image-node-darwin-universal": "0.1.0"
  },
  "repository": "https://github.com/ming900518/pdf-to-image-node",
  "description": "不依賴 canvas 的 PDF 轉圖片 Node.js Library"
}